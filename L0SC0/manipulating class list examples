(defn lc_nthlast
"Returns true if the nth element of lst1 equals the last element of lst2"
[lst1 lst2 n]
(= (nth lst1 n) (last lst2))
)

;;examples that have been copy and pasted
`(s,a) `(t.n) 1)
=> false

(lc_nthlast `(i, a, n) `(n, i, c, h, o, l, s) 2)
   
=> false

(lc_nthlast `(a p p l e) `(o r a n g e) 4)
   
=> true
